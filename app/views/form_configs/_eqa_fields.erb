<fieldset>
<%= f.select("test_codes_id", TestCode.all.collect {|p| [ p.testExpansion, p.id ] }, {:include_blank => 'None'}) %>
<%= f.select("eqa_scheme_id", EqaScheme.all.collect {|p| [ p.name, p.id ] }, {:include_blank => 'None'}) %>
<%= f.select("analyser_id", Analyser.all.collect {|p| [ p.AnalyserName, p.id ] }, {:include_blank => 'None'}) %>
<%= f.text_field :bias,  :placeholder => "bias", :class => "input-mini" %>
<%= f.text_field :dateOfEQA,  :placeholder => "Date of EQA", :class => "input-mini" %>  
<%= f.hidden_field :_destroy %>
<%= link_to "remove", '#', class: "remove_fields" %>
</fieldset>