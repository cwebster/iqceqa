<h1>Listing form_configs</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @form_configs.each do |form_config| %>
  <tr>
    <td><%= link_to 'Show', form_config, :class=>"btn btn-primary" %></td>
    <td><%= link_to 'Edit', edit_form_config_path(form_config), , :class=>"btn btn-success" %></td>
    <td><%= link_to 'Destroy', form_config, method: :delete, data: { confirm: 'Are you sure?' }, :class=>"btn btn-danger" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Form config', new_form_config_path %>
