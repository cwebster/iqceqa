<%= form_tag('/iqc_data/test_view') do -%>
<legend>View IQC Results </legend>
<fieldset>
	<div class="control-group">
		<%= label_tag(:iqc_datum_test_code_id, "Test Name", :class =>"control-label") %>
		<div class="controls">
			<%= select("", "test_code_id", TestCode.all.collect {|p| [ p.testExpansion, p.id ] }, {:include_blank => 'None'}) %>
		</div>
	</div>

	<div class="control-group">
		<%= label_tag(:iqc_datum_iqc_id, "IQC Material", :class =>"control-label") %>
		<div class="controls">
			<%= select("", "iqc_id", Iqc.all.collect {|p| [ p.name, p.id ] }, {:include_blank => 'None'})%>
		</div>
	</div>

	<div class="form-actions">
	<%= submit_tag 'Submit', :class => 'btn-primary'  %>
	</div>
	<fieldset>
<% end %>